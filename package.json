{
  "name": "coinaly",
  "version": "1.0.0",
  "description": "Fast and easy to use mobile trade interface for cryptocurrencies",
  "author": "Jordy van den Aardweg",
  "private": true,
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0",
    "yarn": "1.3.2"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run dev:webpack\" \"npm run dev:api\" \"npm run dev:socket\"",
    "start": "npm run start:all",
    "start:web": "node web/server.js",
    "start:api": "node api/server.js",
    "start:socket": "node socket/server.js",
    "unit": "jest --config web/test/unit/jest.conf.js --coverage",
    "e2e": "node web/test/e2e/runner.js",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue web/src web/test/unit/specs web/test/e2e/specs",
    "build:web": "node web/build/build.js",
    "dev:web": "nodemon web/server.js",
    "dev:api": "nodemon api/server.js",
    "dev:socket": "nodemon socket/server.js",
    "dev:webpack": "webpack-dev-server --inline --progress --config web/build/webpack.dev.conf.js"
  },
